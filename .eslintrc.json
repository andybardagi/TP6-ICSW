{
  "extends": ["eslint:recommended", "next", "next/core-web-vitals", "prettier"],
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "no-undef": "off",
    "no-unused-vars": "off",
    "semi": ["error", "always"],
    "comma-dangle": ["error", "never"]
  }
}
